<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<appSettings>
		<!-- Don't show any update elements -->
    	<!-- Useful for Linux packagers who need control over updates -->
        <add key="ShowUpdate" value="true" />
    	<!-- Used for debugging -->
		<add key="Debug" value="true"/>
		<!-- the file formate to save the options, metainfo, etc -->
		<!-- Excpected file types are xml or dat -->
		<add key="UserFileFormat" value="xml"/>
    </appSettings>
</configuration>